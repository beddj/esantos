#include <stdio.h>
#include <stdlib.h>
#include "node.h"
#include <unistd.h>
#include <string.h>

int ft_list_size(Node_t *start)
{
	int i;
	Node_t *begin;

	begin = start;
	while(begin != NULL)
	{
		i++;
		begin = start->next;
	}
	return (i);
	
}

int main()
{


Node_t *head;
Node_t *temp;

head = (Node_t*)malloc(sizeof(Node_t));
head->next = NULL;
temp = head;

temp->next = (Node_t*)malloc(sizeof(Node_t));
 temp = temp->next;

temp->next = NULL;


printf("%d\n",ft_list_size(head));



}


